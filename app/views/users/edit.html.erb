<%= page_header('Edit Profile') %>

<%= form_for @user do |f| %>
  <%= f.label :company_name  %>
  <%= f.text_field :company_name, class: 'form-control' %>
  <%= f.label :phone  %>
  <%= f.text_field :phone, class: 'form-control' %>
  <%= f.submit "Update profile", class: 'btn btn-primary' %>
<% end %>